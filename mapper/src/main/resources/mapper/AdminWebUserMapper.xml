<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ligg.mapper.AdminWebUserMapper">

    <!--  平台结算金额 -->
    <update id="officialAmountSettlement">
        update admin_web_user
        set money = money + #{commissionAmount}
        where admin_id = #{officialId}
    </update>
</mapper>
